
import numpy as np
import scipy import linalg

A = np.array([[1,3,5], [2,5,1], [2,3,8]])
print(A)
print(linalg.inv(A))  #反矩陣
print(A.dot(linalg.inv(A)))  #矩陣內積
print(la.det(A)) #求行列式值

eig_value, eig_vector = linalg.eig(A)  #求特徵值與對應之特徵向量
eig_value1, eig_value2, eig_value3 = eig_value
print(eig_value1, eig_value2, eig_value3)
